{
    "compilerOptions": {
        "module": "ESNext",
        "target": "ESNext",
        "sourceMap": false,
        "moduleResolution": "Node",
        "allowSyntheticDefaultImports": true,
        "types": [
            "vite/client",
            "@types/node"
        ],
        "paths": {
            "@/*": ["./src/*"],
        }
    },
    "exclude": [
        "node_modules"
    ]
}
